<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.house.dragonfly.admin.register.RegiMapper">

    <!-- 회원 등록 -->
    <insert id="insertMember" parameterType="com.house.dragonfly.domain.MEMBER">
        INSERT INTO member (email, password, tel, name)
        VALUES (#{email}, #{password}, #{tel}, #{name})
    </insert>

</mapper>
